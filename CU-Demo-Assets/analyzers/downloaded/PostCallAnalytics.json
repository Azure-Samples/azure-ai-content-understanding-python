        "items": {
          "type": "string",
          "method": "generate"
        }
      },
      "People": {
        "type": "array",
        "method": "generate",
        "description": "List of people mentioned",
        "items": {
          "type": "object",
          "method": "generate",
          "properties": {
            "Name": {
              "type": "string",
              "method": "generate",
              "description": "Person's name"
            },
            "Role": {
              "type": "string",
              "method": "generate",
              "description": "Person's title/role"
            }
          }
        }
      },
      "Sentiment": {
        "type": "string",
        "method": "classify",
        "description": "Overall sentiment",
        "enum": [
          "Positive",
          "Neutral",
          "Negative"
        ]
      },
      "Categories": {
        "type": "array",
        "method": "generate",
        "description": "List of relevant categories",
        "items": {
          "type": "string",
          "method": "classify",
          "enum": [
            "Agriculture",
            "Business",
            "Finance",
            "Health",
            "Insurance",
            "Mining",
            "Pharmaceutical",
            "Retail",
            "Technology",
            "Transportation"
          ]
